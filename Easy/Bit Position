# Title : Swap Number from CodeEval
# Author : James Fody
# Description : Swap the position of the first and last character

File.open("data").each_line do |line|
  if !line.empty?
    line = line.split(",")

    x = ((Integer(line[0])).to_s(2)).split("")
    y = Integer(line[1])-1
    z = Integer(line[2])-1

    if x[x.length-1-y] == x[x.length-1-z]
      puts true
    else
      puts false
    end

  end
end
